package datastructures.tree;

/**
 * Created by RenatKaitmazov on 22/05/15.
 */
public class PriorityQueue<T extends Comparable> {
    private Tree<T> priorityQueue;

    public PriorityQueue() { priorityQueue = new Tree<>(); }

    public int size() { return priorityQueue.size(); }

    public boolean isEmpty() { return priorityQueue.isEmpty(); }

    public void display() { priorityQueue.displayInOrder(); }

    public void enqueue(T element) { priorityQueue.insert(element); }

    public T dequeue() { return priorityQueue.deleteMax().getData(); }
}
