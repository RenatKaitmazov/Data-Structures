package datastructures.linkedlist;

/**
 * Created by RenatKaitmazov on 06/05/15.
 */

public class Link<T extends Comparable> {
    private Link<T> next;
    private Link<T> previous;
    private T data;

    public Link(T data) {
        this.data = data;
        next = null;
        previous = null;
    }

    public void setNext(Link<T> next) { this.next = next; }
    public Link<T> getNext() { return next; }

    public void setPrevious(Link<T> previous) { this.previous = previous; }
    public Link<T> getPrevious() { return previous; }

    public void setData(T data) { this.data = data; }
    public T getData() { return data; }

    @Override
    public String toString() { return String.format("%s", data); }
}
