public class Link<T> {
    private T element;
    private Link next;

    public Link(T element) {
        this.element = element;
    }

    public void setElement(T element) {
        this.element = element;
    }

    public T getElement() {
        return element;
    }

    public void setNext(Link next) {
        this.next = next;
    }

    public Link getNext() {
        return next;
    }

    @Override
    public String toString() {
        return String.format("%s", element);
    }
}
